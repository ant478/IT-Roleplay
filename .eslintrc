{
  "extends": ["airbnb-base", "plugin:node/recommended"],
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["json"],
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "import/prefer-default-export": "off",
    "node/no-unpublished-require": "off",
    "implicit-arrow-linebreak": "off",
    "function-paren-newline": "off",
    "max-len": "off"
  }
}
